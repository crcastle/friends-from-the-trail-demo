{
  "kafka": {
    "url": "$KAFKA_URL",
    "prefix": "$$KAFKA_PREFIX",
    "submissionTopic": "${self.kafka.prefix}selfie-topic",
    "backgroundTopic": "${self.kafka.prefix}change-background"
  },
  "aws": {
    "id": "$BUCKETEER_AWS_ACCESS_KEY_ID",
    "secret": "$BUCKETEER_AWS_SECRET_ACCESS_KEY",
    "region": "$BUCKETEER_AWS_REGION",
    "bucket": "$BUCKETEER_BUCKET_NAME"
  },
  "region": "$$REGION"
}
